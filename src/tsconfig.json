{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "lib": [
      "dom",
      "es2016",
      "es2017"
    ],
    "module": "es2015",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "paths": {
        "learning-vuex/*": [ "*" ]
    },
    "skipLibCheck": true,
    "sourceMap": false,
    "strictNullChecks": true,
    "target": "es5"
  }
}
