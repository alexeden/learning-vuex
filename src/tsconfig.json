{
  "compilerOptions": {
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "lib": [
      "dom",
      "esnext"
    ],
    "module": "es2015",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "strictPropertyInitialization": true,
    "suppressImplicitAnyIndexErrors": false,
    "paths": {
        "sudoku/*": [ "*" ]
    },
    "skipLibCheck": true,
    "sourceMap": false,
    "strictNullChecks": true,
    "target": "es5"
  }
}
