{
  "name": "learning-vuex",
  "version": "1.0.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexeden/learning-vuex.git"
  },
  "scripts": {
    "builder:type-check": "tsc --noEmit --project build/tsconfig.json",
    "build": "npm run clean && webpack --config ./build/webpack.config.ts",
    "build:watch": "nodemon",
    "lint:scss": "stylelint --config .stylelintrc src/**/*.scss",
    "clean": "rimraf dist",
    "stats": "webpack --config ./build/webpack.config.ts --profile --json > build-stats.json",
    "start": "webpack-dev-server --inline --config ./build/webpack.config.ts"
  },
  "author": "Alex Eden",
  "dependencies": {
    "@types/chalk": "^2.2.0",
    "material-design-icons": "^3.0.1",
    "normalize.css": "^8.0.0",
    "vue": "^2.5.13",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@types/extract-text-webpack-plugin": "^3.0.1",
    "@types/html-webpack-plugin": "^2.30.2",
    "@types/node": "^9.4.6",
    "@types/ramda": "^0.25.18",
    "@types/webpack": "^3.8.8",
    "chalk": "^2.3.1",
    "circular-dependency-plugin": "^4.4.0",
    "clean-webpack-plugin": "^0.1.18",
    "copy-webpack-plugin": "^4.4.1",
    "css-loader": "^0.28.9",
    "extract-loader": "^1.0.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.7.2",
    "nodemon": "^1.15.1",
    "ramda": "^0.25.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.2",
    "stylelint": "^9.1.1",
    "stylelint-config-standard": "^18.1.0",
    "stylelint-scss": "^2.4.0",
    "ts-loader": "^3.5.0",
    "ts-node": "^5.0.0",
    "tslint": "^5.9.1",
    "typescript": "^2.8.0-dev.20180222",
    "url-loader": "^0.6.2",
    "vue-loader": "^14.1.1",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1"
  },
  "nodemonConfig": {
    "ext": "ts,vue",
    "exec": "npm run build",
    "events": {},
    "watch": [
      "src/",
      "build/"
    ]
  }
}
